<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rafaelanastacioalves.stormsecurity">

    <application
        android:name=".StormSecurity_"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:text=".presentation.views.activity.NewsActivity_">
            <intent-filter>
                <action android:text="android.intent.action.MAIN" />

                <category android:text="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
